{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npx functions-framework --target=server --signature-type=http",
    "deploy": "gcloud functions deploy app --region=us-central1 --runtime=nodejs18 --entry-point=server",
    "local:migrate:latest": "npx knex migrate:latest --knexfile database/knexfile.js --env local",
    "local:migrate:rollback": "npx knex migrate:rollback --knexfile database/knexfile.js --env local",
    "gcp:deploy": "gcloud functions deploy app --region=us-central1 --runtime=nodejs16 --entry-point=server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.1.3",
    "autocannon": "^7.10.0"
  },
  "dependencies": {
    "alchemy-sdk": "^2.4.3",
    "axios": "^1.3.0",
    "bookshelf": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "0.21.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.41",
    "path": "^0.12.7",
    "pg": "^8.9.0",
    "swagger-ui-express": "^4.6.2",
    "web3": "^1.8.2"
  }
